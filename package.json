    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@langchain/community": "^0.0.40",
    "@langchain/core": "^0.1.48",
    "@langchain/openai": "^0.0.21",
    "chalk": "^5.3.0",
    "debug": "^4.3.4",
    "dotenv": "^16.4.5",
    "faiss-node": "^0.5.1",
    "langchain": "^0.1.28",
    "llamaindex": "^0.0.0-20240314032004",
    "lodash": "^4.17.21",
    "ora": "^8.0.1",
